<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane prefHeight="170.0" prefWidth="419.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.runApp.LoginController">
    <children>
        <StackPane>
            <children>
                <VBox alignment="CENTER" spacing="10.0" StackPane.alignment="CENTER">
                    <children>
                        <TextField fx:id="UserNameId" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="26.0" prefWidth="196.0" promptText="Username">
                            <VBox.margin>
                                <Insets />
                            </VBox.margin>
                        </TextField>
                        <TextField fx:id="PasswordId" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="26.0" prefWidth="194.0" promptText="Passwoard" />
                        <StackPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="-1.0" prefWidth="419.0">
                            <children>
                                <Button fx:id="loginButtonId" alignment="CENTER" defaultButton="true" mnemonicParsing="false" onAction="#loginButton" text="Login" StackPane.alignment="CENTER_RIGHT" />
                            </children>
                        </StackPane>
                        <Button maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" onAction="#signUpButton" prefWidth="419.0" text="Create account" textFill="#201e1e" />
                    </children>
                </VBox>
            </children>
        </StackPane>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</StackPane>
